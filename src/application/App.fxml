<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>


<AnchorPane prefHeight="340.0" prefWidth="364.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/9.0.1" fx:controller="application.AppController">
   <children>
      <Pane prefHeight="357.0" prefWidth="544.0">
         <children>
            <ToolBar prefHeight="40.0" prefWidth="544.0">
              <items>
                <Button mnemonicParsing="false" onAction="#openAction" text="Open Base File..." />
                  <Button mnemonicParsing="false" onAction="#openRefferFile" text="Reff. File..." />
                  <Label text="Suffix name:" />
                  <TextField fx:id="suffixName" />
                  <Button mnemonicParsing="false" onAction="#execAction" text="Exec..." />
                  <Button mnemonicParsing="false" onAction="#quitAction" text="quit" />
              </items>
            </ToolBar>
            <TextArea fx:id="log" layoutY="40.0" prefHeight="318.0" prefWidth="544.0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
